{
    "name": "cached_node-modules",
    "version": "v1.0.7",
    "description": "A GitHub Action to cache node_modules",
    "main": "./dist/index.js",
    "exports": {
        ".": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "scripts": {
        "lint:check": "eslint src",
        "lint:check-ci": "eslint --exit-on-fatal-error -f @annangela/eslint-formatter-gha --max-warnings 0 ./src",
        "lint:write": "eslint --fix src",
        "lint:eslint.config.flat": "eslint eslint.config.js",
        "package:production": "esbuild src/index.ts --bundle --platform=node --format=esm --legal-comments=inline --outdir=dist --target=node20 --tree-shaking=true --log-limit=0 --minify --analyze",
        "package:development": "esbuild src/index.ts --bundle --platform=node --format=esm --legal-comments=inline --outdir=dist --target=node20 --tree-shaking=true --log-limit=0 --analyze=verbose",
        "package:ncc": "ncc build src/index.ts --out dist --minify",
        "test": "echo \"Error: no test specified\" && exit 1",
        "ci": "rm -rf node_modules && npm ci",
        "tag": "node scripts/newTag.js"
    },
    "engines": {
        "node": "^20.9"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AnnAngela/cached_node-modules.git"
    },
    "keywords": [
        "actions",
        "node",
        "node_modules"
    ],
    "author": "AnnAngela",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AnnAngela/cached_node-modules/issues"
    },
    "homepage": "https://github.com/AnnAngela/cached_node-modules#readme",
    "devDependencies": {
        "@annangela/eslint-formatter-gha": "^1.0.0",
        "@annangela/eslint-plugin-prefer-reflect": "^2.1.3",
        "@eslint/js": "^8.56.0",
        "@stylistic/eslint-plugin": "^1.5.1",
        "@stylistic/eslint-plugin-migrate": "^1.5.1",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "@vercel/ncc": "^0.38.1",
        "esbuild": "^0.19.11",
        "eslint": "^8.56.0",
        "eslint-plugin-prefer-arrow-functions": "^3.2.4",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@actions/cache": "^3.2.2",
        "@actions/core": "^1.10.1",
        "eslint-plugin-n": "^16.6.0",
        "semver": "^7.5.4"
    }
}